*{
	margin:0;
	padding:0;
}
body{
	background-color: red;
	overflow: hidden;
}
.gameContainer{
	background-image: url("bg.jpg");
	background-repeat: no-repeat;
	background-size: 100vw 100vh;
	width: 100%;
	height: 100vh;
}
.sonal{
	background-image: url("girl.png");
	background-repeat: no-repeat;
	background-size: cover;
	width: 263px;
	height: 240px;
	position: absolute;
	bottom: 0;
	left: 70px;
}
.obstacle{
	width: 93px;
	height: 63px;
	background-image: url("bg2.png");
	background-size: cover;
	position: absolute;
	bottom:0;
	left: 44vw;
}	
.animateSonal{
	animation: sonal 0.6s linear;
}
@keyframes sonal{

0%{
	bottom: 0;
}
50%{
	
	bottom:322px;
}
100%{
	bottom: 0;
	
}
}
.gameOver{
	
	position: relative;
	top: 63px;
	font-size: 33px;
	font-weight: bold;
	font-style: underline;
	
	text-align: center;
	font-family: sans-serif;
	
	
	
}
#scoreCont{
	position: absolute;
	right: 45px;
	top : 31px;
	border: 2px solid black;
	padding: 10px;
	width: 112px;
    font-family: sans-serif;
	color: white;
	font-weight: bold;

	
}
.obstacleSnake{
	animation: obstacleSnake 5s linear infinite;
}

@keyframes obstacleSnake{
	
	0%{
		left: 100vw;
	}
	100%{
		left: -10vw;
		1
	}
}

	


